namespace fb;

enum e_commodity : byte {
  food,
  sugar,
  tobacco,
  cotton,
  fur,
  lumber,
  ore,
  silver,
  horses,
  rum,
  cigars,
  cloth,
  coats,
  trade_goods,
  tools,
  muskets
}

struct Commodity {
  type     : e_commodity;
  quantity : int32;
}
